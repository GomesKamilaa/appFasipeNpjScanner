<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

        <View
            android:id="@+id/containerTop"
            android:layout_width="412dp"
            android:layout_height="103dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/background_view_dash"
            app:layout_constraintBottom_toTopOf="@+id/viewListarTodos"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/telaNovoDocumento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="104dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/background_button_dash"
        android:drawableTop="@drawable/ic_createanamnese"
        android:enabled="true"
        android:padding="16dp"
        android:text="@string/txt_novoDocumento"
        android:textAllCaps="false"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/containerTop"/>

    <ListView
        android:id="@+id/viewListarTodos"
        android:layout_width="348dp"
        android:layout_height="371dp"
        android:layout_marginStart="31dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="121dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/containerTop" />




</androidx.constraintlayout.widget.ConstraintLayout>